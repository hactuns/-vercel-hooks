{
  "name": "srvls-hook",
  "version": "1.5.2",
  "description": "Custom hooks for vercel/next serverless API",
  "keywords": [
    "vercel",
    "nextjs",
    "serverless",
    "api",
    "mongodb"
  ],
  "homepage": "https://github.com/hactuns/srvls-hook",
  "bugs": {
    "url": "https://github.com/hactuns/srvls-hook/issues"
  },
  "author": "Hactuns<khactuans.tran@gmail.com>(https://hactuns.me)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hactuns/srvls-hook.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "yarn check-format && yarn check-exports && yarn build"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.0.0",
    "node": ">= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@vercel/node": "^3.2.23",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@vercel/functions": "^1.4.2",
    "cloudwatch-transport": "^1.0.1",
    "mongodb": "^6.9.0",
    "winston": "^3.15.0"
  }
}
