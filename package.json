{
  "name": "function-hooks",
  "version": "0.0.0",
  "description": "Custom hooks for vercel/next serverless API",
  "keywords": [
    "vercel",
    "nextjs",
    "serverless",
    "api"
  ],
  "homepage": "https://github.com/hactuns/vercel-hooks",
  "bugs": {
    "url": "https://github.com/hactuns/vercel-hooks/issues"
  },
  "author": "Hactuns <khactuans.tran@gmail.com> (https://hactun.dev)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hactuns/vercel-hooks.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "yarn check-format && yarn check-exports &&  yarn build"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@vercel/node": "^3.2.23",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "mongodb": "^6.9.0",
    "zod": "^3.23.8"
  }
}
